                  <Label fx:id="errorOfUserName" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="369.0" textFill="RED">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Label>
                  <Label fx:id="bdPass" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="35.0" prefWidth="369.0" text="Password">
